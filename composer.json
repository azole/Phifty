{
    "name": "corneltek\/phifty-core",
    "require": {
        "php": ">=5.4.0",
        "corneltek\/universal": "~1",
        "corneltek\/locale": "~1",
        "corneltek\/universal-cache": "~1",
        "corneltek\/fileutil": "~1",
        "corneltek\/roller": "*",
        "corneltek\/configkit": "~1",
        "corneltek\/assetkit": "*",
        "corneltek\/lazyrecord": "~2",
        "corneltek\/genphp": "*",
        "corneltek\/sessionkit": "~1",
        "corneltek\/actionkit": "dev-master",
        "swiftmailer\/swiftmailer": "@stable",
        "twig\/twig": "~1@stable",
        "twig\/extensions": "*",
        "symfony\/finder": "~2.2@stable",
        "symfony\/process": "~2.2@stable",
        "symfony\/yaml": "~2.2@stable",
        "symfony\/css-selector": "~2.2@stable",
        "michelf\/php-markdown": "~1",
        "corneltek\/pux": "~1"
    },
    "suggest": {
        "corneltek\/twig-markdown": "~0.1",
        "corneltek\/kendo": "~1.0"
    },
    "require-dev": {
        "corneltek\/phpunit-testmore": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "michelf\/php-markdown",
                "version": "1.0",
                "dist": {
                    "url": "https:\/\/github.com\/michelf\/php-markdown\/zipball\/extra",
                    "type": "zip"
                },
                "autoload": {
                    "files": [
                        "markdown.php"
                    ]
                }
            }
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "Phifty": "src"
        }
    },
    "version": "2.10.0"
}
